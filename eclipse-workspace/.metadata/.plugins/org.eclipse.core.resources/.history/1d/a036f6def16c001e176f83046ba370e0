

class AccountHodler{
	private int no;
	private String name;
	private int balance;
	
	
	//Parameterized Constructor 
	public AccountHodler(int no, String name, int balance) {
		this.no = no;
		this.name = name;
		this.balance = balance;
	}

	public AccountHodler() {
		no = 0;
		name = "None";
		balance = 0;
	}
	
	public int getNo() {
		return no;
	}

	public void setNo(int no) {
		this.no = no;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getBalance() {
		return balance;
	}

	public void setBalance(int balance) {
		this.balance = balance;
	}

	public String toString() {
		return "no=" + no + ", name=" + name + ", balance=" + balance;
	}
	
	
}



public class A3Q1 {
	public static void main(String[] args) {
		
		AccountHodler ac1 = new AccountHodler(1,"Ankush",2000);
		System.out.println(ac1);
		
		
	}

	

}
