
public class if_else {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num=11;
		if(num%2==0)
			System.out.println("Even..");
		else
			System.out.println("Odd..");
		
		int n=5, cnt=1;
		while(cnt<=10)
		{
			System.out.print(n*cnt+" ");
			cnt++;
		}
		System.out.println();
		
		for(int i=1;j<=10;i++)
			System.out.print(n*i+" ");

	}

}
