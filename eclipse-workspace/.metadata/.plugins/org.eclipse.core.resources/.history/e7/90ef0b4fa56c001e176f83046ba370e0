public class AccountHolder {
	private int acc_no;
	private String name;
	private int Bal;
	
	public AccountHolder(int acc_no, String name, int bal) {
		this.acc_no = acc_no;
		this.name = name;
		Bal = bal;
	}
	
	public AccountHolder() {
		this(0,"None",000);
	}
	
	
	
}



