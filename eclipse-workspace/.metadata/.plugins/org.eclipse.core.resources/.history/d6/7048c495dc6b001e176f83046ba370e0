
public class Reverse_Array {

	public static void main(String[] args) {
		int [] a = {1,5,2,4,5,2,5};
		System.out.println("Normal Array : ");
		for(int val:a)
			System.out.print(val+" ");
		System.out.println("Reversed Array : ");
		for(int i=a.length-1;i>-1;i--) {
			System.out.print(a[i]+" ");
		}

	}

}
